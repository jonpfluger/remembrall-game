pellList = ({ spells, isLoggedInUser = false }) => {\n  const [removeSpell, { error }] = useMutation(REMOVE_SPELL, {\n    update(cache, { data: { removeSpell } }) {\n      try {\n        cache.writeQuery({\n          query: QUERY_ME,\n          data: { me: removeSpell },\n        });\n      } catch (e) {\n        console.error(e);\n      }\n    },\n  });\n\n  const handleRemoveSpell = async (spell) => {\n    try {\n      const { data } = await removeSpell({\n        variables: { spell },\n      });\n    } catch (err) {\n      console.error(err);\n    }\n  };\n\n  if (!spells.length) {\n    return <h3>No Spells Yet</h3>;\n  }\n\n  return (\n    <div>\n      <div className=\"flex-row justify-space-between my-4\">\n        {spells &&\n          spells.map((spell) => (\n            <div key={spell} className=\"col-12 col-xl-6\">\n              <div className=\"card mb-3\">\n                <h4 className=\"card-header bg-dark text-light p-2 m-0 display-flex align-center\">\n                  <span>{spell}</span>\n                  {isLoggedInUser && (\n                    <button\n                      className=\"btn btn-sm btn-danger ml-auto\"\n                      onClick={() => handleRemoveSpell(spell)}\n                    >\n                      X\n                    </button>\n                  )}\n                </h4>\n              </div>\n            </div>\n          ))}\n      </div>\n      {error && (\n        <div className=\"my-3 p-3 bg-danger text-white\">{error.message}</div>\n      )}\n    </div>\n  );\n};\n\nexport default SpellList;\n","C:\\Users\\jajon\\Desktop\\Projects\\remembrall-game\\client\\src\\components\\SpellForm\\index.js",["78"],"import React, { useState } from 'react';\nimport { Link } from 'react-router-dom';\nimport { useMutation } from '@apollo/client';\n\nimport { ADD_SPELL } from '../../utils/mutations';\n\nimport Auth from '../../utils/auth';\n\nconst SpellForm = ({ wizardId }) => {\n  const [spell, setSpell] = useState('');\n\n  const [addSpell, { error }] = useMutation(ADD_SPELL);\n\n  const handleFormSubmit = async (event) => {\n    event.preventDefault();\n\n    try {\n      const data = await addSpell({\n        variables: { wizardId, spell },\n      });\n\n      setSpell('');\n    } catch (err) {\n      console.error(err);\n    }\n  };\n\n  return (\n    <div>\n      <h4>Endorse some more spells below.</h4>\n\n      {Auth.loggedIn() ? (\n        <form\n          className=\"flex-row justify-center justify-space-between-md align-center\"\n          onSubmit={handleFormSubmit}\n        >\n          <div className=\"col-12 col-lg-9\">\n            <input\n              placeholder=\"Endorse some spells...\"\n              value={spell}\n              className=\"form-input w-100\"\n              onChange={(event) => setSpell(event.target.value)}\n            />\n          </div>\n\n          <div className=\"col-12 col-lg-3\">\n            <button className=\"btn btn-info btn-block py-3\" type=\"submit\">\n              Endorse Spell\n            </button>\n          </div>\n          {error && (\n            <div className=\"col-12 my-3 bg-danger text-white p-3\">\n              {error.message}\n            </div>\n          )}\n        </form>\n      ) : (\n        <p>\n          You need to be logged in to endorse spells. Please{' '}\n          <Link to=\"/login\">login</Link> or <Link to=\"/signup\">signup.</Link>\n        </p>\n      )}\n    </div>\n  );\n};\n\nexport default SpellForm;\n",{"ruleId":"79","replacedBy":"80"},{"ruleId":"81","replacedBy":"82"},{"ruleId":"83","severity":1,"message":"84","line":37,"column":1,"nodeType":"85","endLine":37,"endColumn":34},{"ruleId":"86","severity":1,"message":"87","line":23,"column":15,"nodeType":"88","messageId":"89","endLine":23,"endColumn":19},{"ruleId":"86","severity":1,"message":"87","line":18,"column":13,"nodeType":"88","messageId":"89","endLine":18,"endColumn":17},"no-native-reassign",["90"],"no-negated-in-lhs",["91"],"import/no-ano